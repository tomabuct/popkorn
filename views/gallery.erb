<!doctype html>
<head>
  <title>Gallery</title>
  <script type="text/javascript">
    window.id = "<%= @id %>";
  </script>
  <script src="jquery.js"></script>
  <script src="spin.js"></script>
  <script src="toastr.js"></script>
  <script src="http://static.opentok.com/v0.91/js/TB.min.js"></script>
  <script src="galleria/galleria-1.2.9.js"></script>
  <script src="http://js.pusher.com/1.12/pusher.min.js" type="text/javascript"></script>
  <script src="gallery.js"></script>
  <script src="tok.js"></script>
  <link rel="stylesheet" type="text/css" href="css/pop.css">
  <link rel="stylesheet" type="text/css" href="css/video_style.css">
  <link rel="stylesheet" type="text/css" href="css/style.css">
  <link href="toastr.css" rel="stylesheet"/>
  <style>
    #galleria {
      height: 427px;
      width: 700px;
      margin-right: auto;
      margin-left: auto;
      margin-top: 65px;
    }
  </style>
</head>
<body>
  <h1>popkorn</h1>
  <div id="galleria"> 
    <% for url in @urls %>
      <img src="<%= url %>">
    <% end %>
  </div>
  <div id="footer">
    <div id="publisher"></div>
    <div id="toolbar">
    <div class="icon video" style="position:relative;top:30px;"><img id="videor" style="position:relative;top:-10px;" src="../images/video_chat.png"></img ></div>
    </div>
    <div id="spinner"></div>
  </div>
  <div id="link">Copy & send.</div>
</body>
</html>
